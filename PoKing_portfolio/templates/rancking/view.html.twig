{% extends 'base.html.twig' %}

{% block title %}View Rancking{% endblock %}

{% block body %}
    <h1>Rancking {{ rancking.label }}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ rancking.id }}</td>
            </tr>
            <tr>
                <th>Season</th>
                <td>{{ rancking.season }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ rancking.label }}</td>
            </tr>
            <tr>
                <th>Points</th>
                <td>{{ rancking.points }}</td>
            </tr>
            <tr>
                <th>RankingPosition</th>
                <td>{{ rancking.rankingPosition }}</td>
            </tr>
            <tr>
                <th>EliminatedBy</th>
                <td>{{ rancking.eliminatedBy }}</td>
            </tr>
            <tr>
                <th>KillsNumber</th>
                <td>{{ rancking.killsNumber }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_rancking_update', {'id': rancking.id}) }}">update</a>

    {{ include('rancking/_delete_form.html.twig') }}
{% endblock %}
